{
  "name": "node-xmrig",
  "version": "1.0.0",
  "description": "Node.js wrapper for XMRig - High performance Monero (XMR) CPU miner with full TypeScript support",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "monero",
    "xmr",
    "mining",
    "miner",
    "cryptocurrency",
    "cpu",
    "nodejs",
    "typescript",
    "randomx",
    "xmrig"
  ],
  "scripts": {
    "test": "node test.js",
    "test:ci": "node test-ci.js",
    "install": "node-gyp rebuild",
    "build": "node-gyp configure && node-gyp build",
    "build:win": "node-gyp configure --arch=x64 --dist-url=https://nodejs.org/dist/ && node-gyp build --arch=x64",
    "build:linux": "node-gyp configure --arch=x64 --dist-url=https://nodejs.org/dist/ && node-gyp build --arch=x64",
    "build:mac": "node-gyp configure --arch=x64 --dist-url=https://nodejs.org/dist/ && node-gyp build --arch=x64",
    "build:arm": "node-gyp configure --arch=arm64 --dist-url=https://nodejs.org/dist/ && node-gyp build --arch=arm64",
    "build:x86": "node-gyp configure --arch=ia32 --dist-url=https://nodejs.org/dist/ && node-gyp build --arch=ia32",
    "clean": "node-gyp clean",
    "rebuild": "npm run clean && npm run build",
    "rebuild:win": "npm run clean && npm run build:win",
    "rebuild:linux": "npm run clean && npm run build:linux",
    "rebuild:mac": "npm run clean && npm run build:mac",
    "dev": "npm run build && node examples/basic.js",
    "example:basic": "node examples/basic.js",
    "example:config": "node examples/config.js",
    "example:events": "node examples/events.js",
    "run-script:env": "node -e \"console.log('Node.js:', process.version); console.log('Platform:', process.platform); console.log('Architecture:', process.arch); console.log('CPU cores:', require('os').cpus().length);\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teddy1565/node-xmrig.git"
  },
  "author": {
    "name": "Teddy",
    "email": "teddy1565@github.com"
  },
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/teddy1565/node-xmrig/issues"
  },
  "homepage": "https://github.com/teddy1565/node-xmrig#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "nan": "^2.25.0",
    "node-addon-api": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "cpu": [
    "x64",
    "x86",
    "arm64"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "lib/",
    "examples/",
    "README.md"
  ],
  "directories": {
    "example": "examples",
    "lib": "lib"
  }
}
